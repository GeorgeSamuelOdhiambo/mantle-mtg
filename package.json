{
  "name": "mtg-downloader",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./ -s --copy-files --ignore \"node_modules,.git,.ionide,bin\"  --no-copy-ignored",
    "build": "npm run clean && npm run build-babel"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "adm-zip": "^0.5.9",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "ciql-json": "^1.9.6",
    "config": "^3.3.6",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "node-cron": "^3.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-alpha.1",
    "store2": "^2.12.0",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@babel/node": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.16.3",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "downloads/*",
      "uploads/*",
      "config/doc_result.json"
    ]
  }
}
